/* Custom Styles */
        ul.nav-tabs{
        width: 140px;
        margin-top: 20px;
        border-radius: 4px;
        border: 1px solid $bgcolor;
        }
        ul.nav-tabs li{
        margin: 0;
        }
        ul.nav-tabs li:first-child{
        border-top: none;
        }
        ul.nav-tabs li a{
        margin: 0;
        padding: 8px 16px;
        border-radius: 0;
        }
        ul.nav-tabs li.active a, ul.nav-tabs li.active a:hover{
        color: #fff;
        background: $bgcolor;
        border: 1px solid $bgcolor;
        }
        ul.nav-tabs li:first-child a{
        border-radius: 4px 4px 0 0;
        }
        ul.nav-tabs li:last-child a{
        border-radius: 0 0 4px 4px;
        }
        ul.nav-tabs.affix{
        top: 30px; /* Set the top position of pinned element */
        }
        
        
        .title {font-size:0.4rem;}
        .barrage-area{
        font-size:0.28rem;
        line-height:2em;
        overflow:hidden;
        position:relative;
        height:300px;
        background:url("../images/team_customer.png") center no-repeat ;
        margin:10px;
        list-style: none;
        padding: 0px;
        }

        .barrage-item{
        position:absolute;
        left:100%;
        line-height:2em;
        font-size:14px;
        padding:0px 1em;
        border-radius:100px;
        white-space: nowrap;
        }

        .barrage-item[data-rank = "0"]{
        top:10%;
        }
        .barrage-item[data-rank = "1"]{
        top:30%;
        }
        .barrage-item[data-rank = "2"]{
        top:50%;
        }
        .barrage-item[data-rank = "3"]{
        top:70%;
        }
        .barrage-red{
            color:#FF3030;
            border:1px solid #FF3030;
        }
        .barrage-green{
        color:#9BCD9B;
        border:1px solid #9BCD9B;
        }
        .barrage-blue{
        color:#1874CD;
        border:1px solid #1874CD;
        }
        .barrage-yellow{
        color:#FF8C00;
        border:1px solid #FF8C00;
        }
        .barrage-transparent{
        color:transparent;
        border:1px solid transparent;
        }
        
        .bgColor{
        	background-color:$bgcolor;
        	color:white;
        }

